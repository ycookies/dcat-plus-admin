{
    "name": "dcat-plus/laravel-admin",
    "description": "dcat-plus admin",
    "type": "library",
    "keywords": ["dcat","saas", "laravel", "admin", "grid", "form", "laravel saas admin"],
    "homepage": "https://github.com/ycookies/dcat-plus-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "dcat-plus.saishiyun.net",
            "email": "3664839@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "~8.0|~9.0|~10.0|~11.0",
        "spatie/eloquent-sortable": "3.*|4.*",
        "doctrine/dbal": "^2.6|^3.0"
    },
    "require-dev": {
        "laravel/dusk": "~5.9|~6",
        "phpstan/phpstan": "^0.12.0",
        "phpunit/phpunit": "^7.5|~9",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Dcatplus\\Admin\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dcatplus\\Admin\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcatplus\\Admin\\AdminServiceProvider"
            ]
        }
    }
}
